@model IEnumerable<Hospital.Models.MModels>;
<div class="mt-2" style="width:50%">
    <div class="row">
        <div class="col-md-4">
            <a href="/Medical/MAdd" class="btn btn-primary">AddMedical Reports</a>
        </div>
        <table class="table table-striped" style="margin-left:250px;border:solid;color:black">
            <thead>
                <tr>
                    <th>Patient Name</th>
                    <th>Date & Time </th>
                    <th>DiaGnosis</th>
                    <th>Medications</th>
                    <th>Action</th>
                </tr>

                @foreach (var MModels in Model)
                {
                    <tr>
                        <td>@MModels.PatientName</td>
                        <td>@MModels.Date</td>
                        <td>@MModels.DiaGnosis</td>
                        <td>@MModels.Medications</td>

                        <td><a href="/Medical/MUpdate?Id=@MModels.Id">Edit</a><a href="/Medical/MDelete?Id=@MModels.Id">Delete</a></td>
                    </tr>

                }


            </thead>
        </table>
    </div>
</div>
